	<!--@ C1DataGrid Background Brush -->
	<SolidColorBrush  x:Key="C1DataGrid_Brush" PresentationOptions:Freeze="True" Color="#FFFFFFFF" />
	
    <!--@ Cell Background Brush -->
	<SolidColorBrush  x:Key="C1DataGrid_Cell_Brush" PresentationOptions:Freeze="True" Color="Transparent" />
	<!--@ Cell Background Brush on Mouseover -->		
	<SolidColorBrush  x:Key="C1DataGrid_Cell_HoverBrush" PresentationOptions:Freeze="True" Color="Transparent" />
	<!--@ Selected Cell Background Brush -->
	<LinearGradientBrush  x:Key="C1DataGrid_Cell_SelectedBrush" PresentationOptions:Freeze="True" EndPoint="0,1" StartPoint="0,0" >
		<GradientStop Color="#FFFFF5CC" Offset="0"/>
		<GradientStop Color="#FFFFDC7A" Offset="1"/>
	</LinearGradientBrush>		
	<!--@ Selected Cell Background Brush on Mouseover -->
	<VisualBrush x:Key="C1DataGrid_Cell_SelectedHoverBrush" >
		<VisualBrush.Visual>
			<Grid>
				<Rectangle Stroke="{x:Null}" Width="1" Height="1">
					<Rectangle.Fill>
						<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
							<GradientStop Color="#FFFFF5CC" Offset="0"/>
							<GradientStop Color="#FFFFDC7A" Offset="1"/>
						</LinearGradientBrush>
					</Rectangle.Fill>
				</Rectangle>
				<Rectangle Stroke="{x:Null}" Width="1" Height="1">
					<Rectangle.Fill>
						<RadialGradientBrush>
							<RadialGradientBrush.RelativeTransform>
								<TransformGroup>
									<ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="2" ScaleY="2.255"/>
									<SkewTransform AngleX="0" AngleY="0" CenterX="0.5" CenterY="0.5"/>
									<RotateTransform Angle="0" CenterX="0.5" CenterY="0.5"/>
									<TranslateTransform X="0" Y="0.372"/>
								</TransformGroup>
							</RadialGradientBrush.RelativeTransform>
							<GradientStop Color="#FFFFBD69" Offset="0"/>
							<GradientStop Color="#00FFFFFF" Offset="1"/>
						</RadialGradientBrush>
					</Rectangle.Fill>
				</Rectangle>
			</Grid>
		</VisualBrush.Visual>
	</VisualBrush>	
