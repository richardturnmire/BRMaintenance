	<!--@ Child List Toggle Column Header Background Brush -->
	<LinearGradientBrush  x:Key="C1DataGrid_ChildToggleHeader_Brush" PresentationOptions:Freeze="True" EndPoint="0,1" StartPoint="0,0" >
		<GradientStop Color="#FF626c88" Offset="0"/>
		<GradientStop Color="#FF3f4756" Offset="0.5" />
		<GradientStop Color="#FF151516" Offset="0.55" />
		<GradientStop Color="#FF121727" Offset="0.9"/>	
		<GradientStop Color="#FF3c476f" Offset="1" />
	</LinearGradientBrush>	
	<!--@ Child List Toggle Column Header Background Brush on Mouseover -->
	<VisualBrush x:Key="C1DataGrid_ChildToggleHeader_HoverBrush" >
		<VisualBrush.Visual>
			<Grid>
				<Rectangle Stroke="{x:Null}" Width="1" Height="1">
					<Rectangle.Fill>
						<LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#FF9197a8" Offset="0"/>
		<GradientStop Color="#FF515a6b" Offset="0.5" />
		<GradientStop Color="#FF151b26" Offset="0.55" />
		<GradientStop Color="#FF161f2e" Offset="1" />
						</LinearGradientBrush>
					</Rectangle.Fill>
				</Rectangle>
				<Rectangle Stroke="{x:Null}" Width="1" Height="1">
					<Rectangle.Fill>
						<RadialGradientBrush>
							<RadialGradientBrush.RelativeTransform>
								<TransformGroup>
									<ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="1" ScaleY="2.255"/>
									<SkewTransform AngleX="0" AngleY="0" CenterX="0.5" CenterY="0.5"/>
									<RotateTransform Angle="0" CenterX="0.5" CenterY="0.5"/>
									<TranslateTransform X="0" Y="0.372"/>
								</TransformGroup>
							</RadialGradientBrush.RelativeTransform>
							<GradientStop Color="#FF80d1f9" Offset="0"/>
							<GradientStop Color="#002a5798" Offset="1"/>
						</RadialGradientBrush>
					</Rectangle.Fill>
				</Rectangle>
			</Grid>
		</VisualBrush.Visual>
	</VisualBrush>
